<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Nullable>enable</Nullable>
    <PackageId>Basionix.BaseEntities</PackageId>
    <Version>1.0.0</Version>
    <Authors>Cezar Augustus Lamann</Authors>
    <Company></Company>
    <Product>Basionix Base Entities</Product>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <NeutralLanguage>en</NeutralLanguage>
    <Description>Base classes and interfaces for applications done in C#, so you don't have to create them by hand on every single project you start.</Description>
    <Copyright></Copyright>
    <PackageLicenseUrl>https://github.com/cezarlamann/basionix-base-entities/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/cezarlamann/basionix-base-entities</PackageProjectUrl>
    <PackageIconUrl></PackageIconUrl>
    <RepositoryUrl>https://github.com/cezarlamann/basionix-base-entities</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags></PackageTags>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup> 

</Project>
